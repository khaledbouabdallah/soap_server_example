<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_SolvencyVerification" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.41.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.0.0">
  <bpmn:collaboration id="Collaboration_SolvencyVerification">
    <bpmn:participant id="Participant_Orchestrator" name="Solvency Verification Orchestrator" processRef="Process_Orchestrator" />
    <bpmn:textAnnotation id="Annotation_Formula">
      <bpmn:text>Credit Score Formula:
score = 1000 - (0.1 × debt) - (50 × latePayments) - (hasBankruptcy ? 200 : 0)
Range: [0, 1000]</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:association id="Association_Formula" sourceRef="Annotation_Formula" targetRef="Task_ComputeScore" />
    <bpmn:textAnnotation id="Annotation_SLA">
      <bpmn:text>SLA Targets (Client-Facing):
- P95 Latency: &lt;300ms
- P50 Latency: &lt;60ms
- Availability: 99% uptime
- Cache reduces latency by 40-60%</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:association id="Association_SLA" sourceRef="Annotation_SLA" targetRef="Task_StartTimer" />
    <bpmn:textAnnotation id="Annotation_Cache">
      <bpmn:text>Cache Configuration:
- TTL: 5 minutes (300s)
- Max Size: 1000 entries
- Eviction: LRU policy
- Keys: identity:{id}, financials:{id}, history:{id}
- Hit Rate Target: &gt;70%</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:association id="Association_Cache" sourceRef="Annotation_Cache" targetRef="Gateway_ParallelSplit" />
  </bpmn:collaboration>
  <bpmn:process id="Process_Orchestrator" name="VerifySolvency Orchestration" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Receive VerifySolvency Request">
      <bpmn:outgoing>Flow_01</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:serviceTask id="Task_GenerateCorrelationID" name="Generate Correlation ID (UUID)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="generate-correlation-id" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_01</bpmn:incoming>
      <bpmn:outgoing>Flow_02</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_ValidateClientID" name="Validate Client ID Pattern (client-\d{3})">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="validate-client-id" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_02</bpmn:incoming>
      <bpmn:outgoing>Flow_03</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_ValidPattern" name="Valid Pattern?">
      <bpmn:incoming>Flow_03</bpmn:incoming>
      <bpmn:outgoing>Flow_04_Yes</bpmn:outgoing>
      <bpmn:outgoing>Flow_04_No</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:endEvent id="EndEvent_ValidationError" name="Throw ClientValidationError">
      <bpmn:incoming>Flow_04_No</bpmn:incoming>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_1" errorRef="Error_ValidationError" />
    </bpmn:endEvent>
    <bpmn:serviceTask id="Task_StartTimer" name="Start Performance Timer">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="start-timer" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_04_Yes</bpmn:incoming>
      <bpmn:outgoing>Flow_05</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:parallelGateway id="Gateway_ParallelSplit" name="Parallel CRUD Retrieval">
      <bpmn:incoming>Flow_05</bpmn:incoming>
      <bpmn:outgoing>Flow_06a</bpmn:outgoing>
      <bpmn:outgoing>Flow_06b</bpmn:outgoing>
      <bpmn:outgoing>Flow_06c</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:serviceTask id="Task_GetIdentity" name="Get Client Identity&#10;(Check Cache → CRUD Service → Store)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="get-client-identity" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_06a</bpmn:incoming>
      <bpmn:outgoing>Flow_07a</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:boundaryEvent id="BoundaryError_Identity" name="Client Not Found" attachedToRef="Task_GetIdentity">
      <bpmn:outgoing>Flow_Error1</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_Identity" errorRef="Error_NotFound" />
    </bpmn:boundaryEvent>
    <bpmn:serviceTask id="Task_GetFinancials" name="Get Client Financials&#10;(Check Cache → CRUD Service → Store)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="get-client-financials" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_06b</bpmn:incoming>
      <bpmn:outgoing>Flow_07b</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:boundaryEvent id="BoundaryError_Financials" name="Client Not Found" attachedToRef="Task_GetFinancials">
      <bpmn:outgoing>Flow_Error2</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_Financials" errorRef="Error_NotFound" />
    </bpmn:boundaryEvent>
    <bpmn:serviceTask id="Task_GetHistory" name="Get Credit History&#10;(Check Cache → CRUD Service → Store)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="get-credit-history" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_06c</bpmn:incoming>
      <bpmn:outgoing>Flow_07c</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:boundaryEvent id="BoundaryError_History" name="Client Not Found" attachedToRef="Task_GetHistory">
      <bpmn:outgoing>Flow_Error3</bpmn:outgoing>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_History" errorRef="Error_NotFound" />
    </bpmn:boundaryEvent>
    <bpmn:sequenceFlow id="Flow_Error1" sourceRef="BoundaryError_Identity" targetRef="Gateway_ErrorMerge" />
    <bpmn:sequenceFlow id="Flow_Error2" sourceRef="BoundaryError_Financials" targetRef="Gateway_ErrorMerge" />
    <bpmn:sequenceFlow id="Flow_Error3" sourceRef="BoundaryError_History" targetRef="Gateway_ErrorMerge" />
    <bpmn:sequenceFlow id="Flow_Error4" sourceRef="Gateway_ErrorMerge" targetRef="Task_LogError" />
    <bpmn:sequenceFlow id="Flow_Error5" sourceRef="Task_LogError" targetRef="EndEvent_NotFound" />
    <bpmn:parallelGateway id="Gateway_ParallelJoin" name="All Data Retrieved">
      <bpmn:incoming>Flow_07a</bpmn:incoming>
      <bpmn:incoming>Flow_07b</bpmn:incoming>
      <bpmn:incoming>Flow_07c</bpmn:incoming>
      <bpmn:outgoing>Flow_08</bpmn:outgoing>
    </bpmn:parallelGateway>
    <bpmn:serviceTask id="Task_RecordCRUDMetrics" name="Record CRUD Metrics (latency)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="record-crud-metrics" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_08</bpmn:incoming>
      <bpmn:outgoing>Flow_09</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_ComputeScore" name="Call ComputeCreditScore">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="compute-credit-score" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_09</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_RecordScoreMetric" name="Record Score Metric">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="record-score-metric" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_10</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_MakeDecision" name="Call DecideSolvency&#10;Rule: (score ≥ 700) AND (income &#62; expenses)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="decide-solvency" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_11</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_RecordDecisionMetric" name="Record Decision Metric">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="record-decision-metric" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_13</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_GenerateExplanations" name="Call Explain&#10;Generate 3 Explanations">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="generate-explanations" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_13</bpmn:incoming>
      <bpmn:outgoing>Flow_14</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_RecordExplanationMetric" name="Record Explanation Metric">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="record-explanation-metric" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_14</bpmn:incoming>
      <bpmn:outgoing>Flow_15</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_01" sourceRef="StartEvent_1" targetRef="Task_GenerateCorrelationID" />
    <bpmn:sequenceFlow id="Flow_02" sourceRef="Task_GenerateCorrelationID" targetRef="Task_ValidateClientID" />
    <bpmn:sequenceFlow id="Flow_03" sourceRef="Task_ValidateClientID" targetRef="Gateway_ValidPattern" />
    <bpmn:sequenceFlow id="Flow_04_Yes" name="YES" sourceRef="Gateway_ValidPattern" targetRef="Task_StartTimer">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=patternValid = true</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_04_No" name="NO" sourceRef="Gateway_ValidPattern" targetRef="EndEvent_ValidationError">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=patternValid = false</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_05" sourceRef="Task_StartTimer" targetRef="Gateway_ParallelSplit" />
    <bpmn:sequenceFlow id="Flow_06a" sourceRef="Gateway_ParallelSplit" targetRef="Task_GetIdentity" />
    <bpmn:sequenceFlow id="Flow_06b" sourceRef="Gateway_ParallelSplit" targetRef="Task_GetFinancials" />
    <bpmn:sequenceFlow id="Flow_06c" sourceRef="Gateway_ParallelSplit" targetRef="Task_GetHistory" />
    <bpmn:sequenceFlow id="Flow_07a" sourceRef="Task_GetIdentity" targetRef="Gateway_ParallelJoin" />
    <bpmn:sequenceFlow id="Flow_07b" sourceRef="Task_GetFinancials" targetRef="Gateway_ParallelJoin" />
    <bpmn:sequenceFlow id="Flow_07c" sourceRef="Task_GetHistory" targetRef="Gateway_ParallelJoin" />
    <bpmn:sequenceFlow id="Flow_08" sourceRef="Gateway_ParallelJoin" targetRef="Task_RecordCRUDMetrics" />
    <bpmn:sequenceFlow id="Flow_09" sourceRef="Task_RecordCRUDMetrics" targetRef="Task_ComputeScore" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Task_ComputeScore" targetRef="Task_RecordScoreMetric" />
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Task_RecordScoreMetric" targetRef="Task_MakeDecision" />
    <bpmn:sequenceFlow id="Flow_12" sourceRef="Task_MakeDecision" targetRef="Task_RecordDecisionMetric" />
    <bpmn:sequenceFlow id="Flow_13" sourceRef="Task_RecordDecisionMetric" targetRef="Task_GenerateExplanations" />
    <bpmn:sequenceFlow id="Flow_14" sourceRef="Task_GenerateExplanations" targetRef="Task_RecordExplanationMetric" />
    <bpmn:sequenceFlow id="Flow_15" sourceRef="Task_RecordExplanationMetric" targetRef="Task_AssembleReport" />
    <bpmn:sequenceFlow id="Flow_16" sourceRef="Task_AssembleReport" targetRef="Task_CalculateLatency" />
    <bpmn:sequenceFlow id="Flow_17" sourceRef="Task_CalculateLatency" targetRef="Task_RecordTotalMetric" />
    <bpmn:sequenceFlow id="Flow_18" sourceRef="Task_RecordTotalMetric" targetRef="Task_LogCacheStats" />
    <bpmn:sequenceFlow id="Flow_19" sourceRef="Task_LogCacheStats" targetRef="EndEvent_Success" />
    <bpmn:serviceTask id="Task_LogCacheStats" name="Log Cache Statistics (hit rate %)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="log-cache-stats" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_18</bpmn:incoming>
      <bpmn:outgoing>Flow_19</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_ErrorMerge" name="Merge Errors">
      <bpmn:incoming>Flow_Error1</bpmn:incoming>
      <bpmn:incoming>Flow_Error2</bpmn:incoming>
      <bpmn:incoming>Flow_Error3</bpmn:incoming>
      <bpmn:outgoing>Flow_Error4</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_LogError" name="Log Error with&#10;Correlation ID">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="log-error" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Error4</bpmn:incoming>
      <bpmn:outgoing>Flow_Error5</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_NotFound" name="Propagate&#10;ClientNotFoundFault">
      <bpmn:incoming>Flow_Error5</bpmn:incoming>
      <bpmn:errorEventDefinition id="ErrorEventDefinition_NotFoundEnd" errorRef="Error_NotFound" />
    </bpmn:endEvent>
    <bpmn:serviceTask id="Task_CalculateLatency" name="Calculate Total Latency">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="calculate-latency" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_16</bpmn:incoming>
      <bpmn:outgoing>Flow_17</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_AssembleReport" name="Assemble SolvencyReport&#10;(identity + financials + history + score + status + explanations)">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="assemble-report" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_15</bpmn:incoming>
      <bpmn:outgoing>Flow_16</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_RecordTotalMetric" name="Record VerifySolvency Metric">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="record-total-metric" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_17</bpmn:incoming>
      <bpmn:outgoing>Flow_18</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_Success" name="Return SolvencyReport">
      <bpmn:incoming>Flow_19</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:textAnnotation id="Annotation_Decision">
      <bpmn:text>Solvency Decision Rule:
solvent = (score ≥ 700) AND (monthlyIncome &gt; monthlyExpenses)
Both conditions must be TRUE</bpmn:text>
    </bpmn:textAnnotation>
    <bpmn:association id="Association_Decision" sourceRef="Annotation_Decision" targetRef="Task_MakeDecision" />
  </bpmn:process>
